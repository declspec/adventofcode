{
    "compileOnSave": true,
    "compilerOptions": {
        "strict": true,
        "strictNullChecks": true,
        "noImplicitThis": true,
        "noImplicitAny": true,
        "experimentalDecorators": true,
        "emitDecoratorMetadata": true,
        "moduleResolution": "node",
        "target": "es2017",
        "module": "commonjs",
        "outDir": "./bin",
        "lib": [ "es2017" ],
        "importHelpers": true,
        "sourceMap": true
    }
}